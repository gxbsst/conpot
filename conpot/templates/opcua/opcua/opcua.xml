    <!-- 明匠坡口机器人 -->
    <!-- State: waiting -> starting -> delivered -> grooved －> completed -->
    <object node_id="ns=1;s=MJGrooveRobot" browser_name="MJGrooveRobot">
        <variable node_id="ns=1;s=MJGrooveRobot.MovingPro" browser_name="MovingPro" >
